{
  "name": "pacepen",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "gen:types": "npx supabase gen types typescript --local > src/lib/supabase-types.ts && prettier --write src/lib/supabase-types.ts",
    "seed": "tsx --tsconfig scripts/tsconfig.json scripts/seed.ts",
    "stripe:webhooks": "stripe listen --forward-to localhost:5173/api/stripe/webhook"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@playwright/test": "^1.35.1",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/adapter-vercel": "^3.0.1",
    "@sveltejs/kit": "^1.20.4",
    "@types/detect-port": "^1.3.3",
    "@types/pg": "^8.10.5",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "autoprefixer": "^10.4.14",
    "bits-ui": "^0.6.3",
    "detect-port": "^1.5.1",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte": "^2.30.0",
    "pg": "^8.11.3",
    "phosphor-svelte": "^1.3.0",
    "postcss": "^8.4.24",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.10.1",
    "supabase": "^1.100.1",
    "svelte": "^4.2.2",
    "svelte-check": "^3.4.3",
    "sveltekit-superforms": "^1.0.0",
    "tailwindcss": "^3.3.2",
    "tslib": "^2.4.1",
    "tsx": "^3.13.0",
    "typescript": "^5.0.0",
    "vite": "^4.3.0",
    "zod": "^3.21.4"
  },
  "type": "module",
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "@supabase/auth-helpers-sveltekit": "^0.9.4",
    "@supabase/supabase-js": "^2.24.0",
    "classnames": "^2.3.2",
    "clsx": "^2.0.0",
    "dotenv": "^16.1.4",
    "lucide-svelte": "^0.288.0",
    "stripe": "^12.9.0",
